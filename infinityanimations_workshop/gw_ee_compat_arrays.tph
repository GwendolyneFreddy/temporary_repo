 *                                      + Armour Levels (4) + Arbitrary (originally WP) Object anim. + CSHD.           *
 * ------------------------------------------------------------------------------------------------------------------- */
	BG1CHAR,	0x5600,		0,		0,		0x6000	=> 1CH5600
	BG1CHAR,	0x6600,		0,		0,		0x7000	=> 1CH6600
